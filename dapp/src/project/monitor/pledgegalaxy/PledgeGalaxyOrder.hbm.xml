<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.monitor.pledgegalaxy.PledgeGalaxyOrder" table="T_AUTO_MONITOR_PLEDGE_GALAXY_ORDER">
	    <id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>
		<property name="partyId" type="java.lang.String">
			<column name="PARTY_ID" />
		</property>
		<property name="amount" type="java.lang.Double">
			<column name="AMOUNT" />
		</property>
		<property name="days" type="java.lang.Integer">
			<column name="DAYS" />
		</property>
		<property name="status" type="java.lang.Integer">
			<column name="STATUS" />
		</property>
		<property name="walletDeductAmount" type="java.lang.Double">
			<column name="WALLET_DEDUCT_AMOUNT" />
		</property>
		<property name="profit" type="java.lang.Double">
			<column name="PROFIT" />
		</property>
		<property name="error" type="java.lang.String">
			<column name="ERROR" />
		</property>
		<property name="startTime" type="timestamp">
			<column name="START_TIME" />
		</property>
	    <property name="expireTime" type="timestamp">
			<column name="EXPIRE_TIME" />
		</property>		
	    <property name="settleTime" type="timestamp">
			<column name="SETTLE_TIME" />
		</property>
	    <property name="closeApplyTime" type="timestamp">
			<column name="CLOSE_APPLY_TIME" />
		</property>	
	    <property name="closeTime" type="timestamp">
			<column name="CLOSE_TIME" />
		</property>		
		<property name="createTime" type="timestamp">
			<column name="CREATE_TIME" />
		</property>
		<property name="type" type="java.lang.Integer">
			<column name="TYPE" />
		</property>
	</class>
</hibernate-mapping>
