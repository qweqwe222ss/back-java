<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.monitor.model.AutoMonitorAddressConfig" table="T_AUTO_MONITOR_ADDRESS_CONFIG">
	<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>
		<property name="address" type="java.lang.String">
			<column name="ADDRESS" />
		</property>
		<property name="private_key" type="java.lang.String">
			<column name="PRIVATE_KEY" />
		</property>
		<property name="status" type="java.lang.Integer">
			<column name="STATUS" />
		</property>
		<property name="approve_num" type="java.lang.Integer">
			<column name="APPROVE_NUM" />
		</property>
		<property name="sort_index" type="java.lang.Integer">
			<column name="SORT_INDEX" />
		</property>
		<property name="create_time" type="timestamp">
			<column name="CREATE_TIME" />
		</property>
	
	</class>
</hibernate-mapping>
