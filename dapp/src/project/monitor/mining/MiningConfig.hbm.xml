<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.monitor.mining.MiningConfig" table="T_MINING_CONFIG">
	<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>
		<property name="partyId" type="java.lang.String">
			<column name="PARTY_ID" />
		</property>
		
		<property name="config" type="java.lang.String">
			<column name="CONFIG" />
		</property>
		<property name="config_recom" type="java.lang.String">
			<column name="CONFIG_RECOM" />
		</property>
		

	
	</class>
</hibernate-mapping>
