<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.mall.goods.model.SellerGoodsBrowsHistory" table="T_MALL_SELLER_GOODS_BROWS_HISTORY">

		<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>
		<property name="sellerId" type="java.lang.String">
			<column name="SELLER_ID" />
		</property>
		<property name="sellerGoodsId" type="java.lang.String">
			<column name="SELLER_GOODS_ID" />
		</property>
		<property name="times" type="java.lang.Integer">
			<column name="TIMES"/>
		</property>
		<property name="partyId" type="java.lang.String">
			<column name="PARTY_ID"/>
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" />
		</property>


	</class>
</hibernate-mapping>
