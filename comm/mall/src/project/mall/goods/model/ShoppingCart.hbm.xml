<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="project.mall.goods.model.ShoppingCart" table="T_MALL_SHOPPING_CART">

        <id name="id" type="java.lang.String">
            <column name="UUID" />
            <generator class="uuid.hex" />
        </id>
        <property name="partyId" type="java.lang.String">
            <column name="PARTY_ID" />
        </property>
        <property name="goodsId" type="java.lang.String">
            <column name="GOODS_ID" />
        </property>
        <property name="sellerId" type="java.lang.String">
            <column name="SELLER_ID" />
        </property>
        <property name="skuId" type="java.lang.String">
            <column name="SKU_ID" />
        </property>
        <property name="buyNum" type="java.lang.Integer">
            <column name="BUY_NUM" />
        </property>
        <property name="createTime" type="timestamp">
            <column name="CREATE_TIME" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="STATUS" />
        </property>
        <property name="tempId" type="java.lang.String">
            <column name="TEMP_ID" />
        </property>
    </class>
</hibernate-mapping>
