<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.mall.goods.model.Evaluation" table="T_MALL_EVALUATION">

		<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>

		<property name="sellerId" type="java.lang.String">
			<column name="SELLER_ID" />
		</property>

		<property name="sellerGoodsId" type="java.lang.String">
			<column name="SELLER_GOODS_ID" />
		</property>
		<property name="systemGoodsId" type="java.lang.String">
			<column name="SYSTEM_GOODS_ID" />
		</property>
		<property name="userName" type="java.lang.String">
			<column name="USER_NAME" />
		</property>

		<property name="partyId" type="java.lang.String">
			<column name="PARTY_ID" />
		</property>
		<property name="partyName" type="java.lang.String">
			<column name="PARTY_NAME" />
		</property>
		<property name="partyAvatar" type="java.lang.String">
			<column name="PARTY_AVATAR" />
		</property>
		<property name="template" type="java.lang.String">
			<column name="TEMPLATE" />
		</property>
		<property name="sourceType" type="java.lang.Integer">
			<column name="SOURCE_TYPE" />
		</property>
		<property name="skuId" type="java.lang.String">
			<column name="SKU_ID" />
		</property>
		<property name="evaluationTime" type="java.util.Date">
			<column name="EVALUATION_TIME" />
		</property>
		<property name="goodsStatus" type="java.lang.Integer">
			<column name="GOODS_STATUS" />
		</property>


		<property name="rating" type="int">
			<column name="RATING" />
		</property>

		<property name="evaluationType" type="int">
			<column name="EVALUATION_TYPE" />
		</property>


		<property name="orderId" type="java.lang.String">
			<column name="ORDER_ID" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="CREATE_TIME" />
		</property>

		<property name="content" type="java.lang.String">
			<column name="CONTENT" />
		</property>
		<property name="status"  type="int">
			<column name="STATUS" />
		</property>

		<property name="countryId"  type="java.lang.Integer">
			<column name="COUNTRY_ID" />
		</property>

		<property name="imgUrl1"  type="java.lang.String">
			<column name="IMG_URL_1" />
		</property>
		<property name="imgUrl2"  type="java.lang.String">
			<column name="IMG_URL_2" />
		</property>
		<property name="imgUrl3"  type="java.lang.String">
			<column name="IMG_URL_3" />
		</property>
		<property name="imgUrl4"  type="java.lang.String">
			<column name="IMG_URL_4" />
		</property>
		<property name="imgUrl5"  type="java.lang.String">
			<column name="IMG_URL_5" />
		</property>
		<property name="imgUrl6"  type="java.lang.String">
			<column name="IMG_URL_6" />
		</property>
		<property name="imgUrl7"  type="java.lang.String">
			<column name="IMG_URL_7" />
		</property>
		<property name="imgUrl8"  type="java.lang.String">
			<column name="IMG_URL_8" />
		</property>
		<property name="imgUrl9"  type="java.lang.String">
			<column name="IMG_URL_9" />
		</property>

	</class>
</hibernate-mapping>
