<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.mall.goods.model.SystemGoodsLang" table="T_MALL_SYSTEM_GOODS_LANG">
		<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>

		<property name="goodsId"  type="java.lang.String">
			<column name="GOODS_ID" />
		</property>

		<property name="lang"  type="java.lang.String">
			<column name="LANG" />
		</property>

		<property name="unit"  type="java.lang.String">
			<column name="UNIT" />
		</property>
		<property name="name"  type="java.lang.String">
			<column name="NAME" />
		</property>

		<property name="des"  type="java.lang.String">
			<column name="DES" />
		</property>

		<property name="imgDes"  type="java.lang.String">
			<column name="IMG_DES" />
		</property>

		<property name="type"  type="java.lang.Integer">
			<column name="TYPE" />
		</property>
	</class>
</hibernate-mapping>
