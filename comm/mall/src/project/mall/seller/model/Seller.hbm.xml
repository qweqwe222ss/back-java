		<property name="autoValid" type="java.lang.Integer">
			<column name="AUTO_VALID" />
		</property>
		<property name="recTime" type="java.lang.Long">
			<column name="REC_TIME" />
		</property>
		<property name="createTime" type="timestamp">
			<column name="CREATE_TIME" />
		</property>
		<property name="reals" type="java.lang.Integer">
			<column name="REALS" />
		</property>
		<property name="fake" type="java.lang.Integer">
			<column name="FAKE" />
		</property>
		<property name="creditScore" type="java.lang.Integer">
			<column name="CREDIT_SCORE" />
