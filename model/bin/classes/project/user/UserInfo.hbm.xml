<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.user.UserInfo" table="T_PARTY_USER">
		<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>
		<property name="partyId" type="java.lang.String">
			<column name="PARTY_ID" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<property name="secname" type="java.lang.String">
			<column name="SECNAME" />
		</property>
		<property name="email" type="java.lang.String">
			<column name="EMAIL" />
		</property>
		<property name="phone" type="java.lang.String">
			<column name="PHONE" />
		</property>
		<property name="acc_type" type="java.lang.String">
			<column name="ACC_TYPE" />
		</property>
		<property name="openacc_dept" type="java.lang.String">
			<column name="OPENACC_DEPT" />
		</property>
		<property name="acc_no" type="java.lang.String">
			<column name="ACC_NO" />
		</property>
		<property name="acc_name" type="java.lang.String">
			<column name="ACC_NAME" />
		</property>
		<property name="business_licence" type="java.lang.String">
			<column name="BUSINESS_LICENCE" />
		</property>
		<property name="address" type="java.lang.String">
			<column name="ADDRESS" />
		</property>
			<property name="idcard" type="java.lang.String">
			<column name="IDCARD" />
		</property>
		<property name="shopphotos" type="java.lang.String">
			<column name="SHOPPHOTOS" />
		</property>
			<property name="twoPassword" type="java.lang.String">
			<column name="TWOPASSWORD" />
		</property>
		<property name="twoPasswordPhone" type="java.lang.String">
			<column name="TWOPASSWORDPHONE" />
		</property>
	</class>
</hibernate-mapping>
