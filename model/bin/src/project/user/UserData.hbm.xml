<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.user.UserData" table="T_USERDATA">
	<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>
		<property name="partyId" type="java.lang.String">
			<column name="PARTY_ID" />
		</property>
		<property name="amount" type="double">
			<column name="AMOUNT" />
		</property>	
		<property name="recharge" type="double">
			<column name="RECHARGE" />
		</property>	
	
			<property name="withdraw" type="double">
			<column name="WITHDRAW" />
		</property>	
			<property name="fee" type="double">
			<column name="FEE" />
		</property>	
		
		<property name="createTime" type="timestamp">
			<column name="CREATE_TIME" />
		</property>
	
		<property name="order_income" type="double">
			<column name="ORDER_INCOME" />
		</property>	
		
	</class>
</hibernate-mapping>
