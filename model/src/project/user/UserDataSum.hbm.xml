<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="project.user.UserDataSum" table="T_USERDATASUM">
	<id name="id" type="java.lang.String">
			<column name="UUID" />
			<generator class="uuid.hex" />
		</id>
		<property name="partyId" type="java.lang.String">
			<column name="PARTY_ID" />
		</property>
		<property name="recharge_sum" type="double">
			<column name="RECHARGE_SUM" />
		</property>	
		<property name="reco_num" type="int">
			<column name="RECO_NUM" />
		</property>	
	
	</class>
</hibernate-mapping>
